{
  "name": "apollo-boilerplate",
  "version": "1.0.0",
  "main": "src/index.js",
  "license": "MIT",
  "scripts": {
    "start": "nodemon --exec babel-node ./server",
    "build": "babel src --out-dir dist",
    "serve": "node dist/index.js",
    "test": "NODE_ENV=test jest --colors --detectOpenHandles --coverage && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js && rm -rf ./coverage"
  },
  "dependencies": {
    "and": "0.0.3",
    "apollo-server": "2.19.2",
    "apollo-server-core": "2.19.2",
    "apollo-server-express": "2.19.2",
    "bcryptjs": "2.4.3",
    "cloudinary": "1.23.0",
    "dependencies": "0.0.1",
    "dotenv": "8.2.0",
    "dotenv-safe": "8.2.0",
    "eslint-plugin-promise": "4.2.1",
    "express": "4.17.1",
    "graphql": "15.4.0",
    "graphql-iso-date": "3.6.1",
    "graphql-tag": "2.11.0",
    "jsonwebtoken": "8.5.1",
    "moment": "2.29.1",
    "mongoose": "5.11.12",
    "nodemon": "2.0.7",
    "sib-api-v3-sdk": "^8.2.0"
  },
  "devDependencies": {
    "@babel/cli": "7.12.10",
    "@babel/core": "7.12.10",
    "@babel/node": "7.12.10",
    "@babel/plugin-proposal-class-properties": "7.12.1",
    "@babel/plugin-transform-runtime": "7.12.10",
    "@babel/polyfill": "7.12.1",
    "@babel/preset-env": "7.12.11",
    "@babel/runtime": "7.12.5",
    "apollo-server-testing": "2.19.2",
    "coveralls": "3.1.0",
    "eslint": "7.17.0",
    "eslint-config-airbnb": "18.2.1",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jsx-a11y": "6.4.1",
    "eslint-plugin-react": "7.22.0",
    "faker": "5.1.0",
    "jest": "26.6.3",
    "prettier": "2.2.1"
  },
  "jest": {
    "testPathIgnorePatterns": [
      "<rootDir>/server/__tests__/base/"
    ]
  }
}
